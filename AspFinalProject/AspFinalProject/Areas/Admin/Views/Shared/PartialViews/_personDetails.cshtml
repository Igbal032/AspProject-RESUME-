@model AspFinalProject.Models.Entity.Person

<ul class="personal-info">
    <li>
        <p>
            <span>
                <input class="form-control" value="@Model.FullName" placeholder="Name">
            </span>
            <input class="form-control" value="@Model.Age" placeholder="Title">
        </p>
    </li>
    <li>
        <p>
            <span>
                <input class="form-control" value="@Model.Email" placeholder="Title">
            </span>
            <input class="form-control" value="@Model.Experience" placeholder="Title">
        </p>
        <p>
            <span>
                <input class="form-control" value="@Model.CareerLevel" placeholder="Title">
            </span>
        </p>
        <p>
            <span>
                <input class="form-control" value="@Model.Degree" placeholder="Title">
            </span>
        </p>
    </li>
    <li>
        <p>
            <span>
                <input class="form-control" value="@Model.Location" placeholder="Title">
            </span>
            <input class="form-control" value="@Model.Phone" placeholder="Title">
        </p>
       
        <p>
            <span>
                <input class="form-control" value="@Model.Fax" placeholder="Title">
            </span>
            <input class="form-control" value="@Model.Website" placeholder="Title">
        </p>
    </li>
    <li>
        <a href="#" onclick="showDetails()" class="btn"><i class="fa fa-plus"></i> Edit</a>
    </li>
</ul>
