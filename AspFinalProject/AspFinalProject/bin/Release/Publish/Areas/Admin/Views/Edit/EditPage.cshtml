                        <input type="text" class="form-control" id="newNameSkill" aria-describedby="emailHelp" placeholder="Enter Skill Name">

                    </div>
                    <button id="addNewSkill" onclick="AddSkill()" class="btn btn-sm btn-primary">Add</button>
                    <button id="deleteExistSkill" onclick="DeleteExistSkill()" class="btn btn-sm btn-primary">Delete</button>


                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-sm btn-secondary" data-dismiss="modal">Close</button>
                </div>
            </div>
        </div>
