        function AcademicBackground() {

        $.ajax({
            url:" @Url.Action("AddAcademicBacground","Edit", new { area="Admin"})",
            type: "POST",
            data: {
                qualification: $(".qualificationText").val() ,
                degree: $(".degreeText").val() ,
                institute: $(".instituteText").val() ,
                yearofobtention: $(".obtentionText").val()  ,
                detail: $(".detailsText").val(),

            },

            success: function (response) {
                console.log(response)

                $("#alertAboutAcBackgroun").append('<div class="exp"> <div class="media-left"> <span class="sun"> '+
                   response.Qualification+'</span> </div> <div class="media-body"><div class="company-logo"> <span class="diploma"><i class="fa fa-graduation-cap"></i> Download Diploma</span> </div>  <h6>Multimedia Arts</h6> <p>'
                    + response.Degree + '</p>  <p>' + response.UniversityName + '</p>   <p class="margin-top-10"> '
                    +  response.YearOfObtention + '</p>  </div>  </div>')

                $(".qualificationText").val(" ");
                    $(".degreeText").val(" ");
                    $(".instituteText").val(" ");
                    $(".obtentionText").val(" ");
                    $(".detailsText").val(" ");
            },
            error: function (response) {

            }

        })
    }


   function Experience() {

        $.ajax({
            url:" @Url.Action("AddExperience","Edit", new { area="Admin"})",
            type: "POST",
            data: {
                duration: $(".duration").val() ,
                jobtitle: $(".jobtitle").val() ,
                companyname: $(".companyname").val() ,
                location: $(".location").val()  ,
                details: $(".details").val(),

            },

            success: function (response) {
                console.log(response)

                    $("#alertAboutExperience").append('<div class="exp"> <div class="media-left"> <span class="sun"> '+
                  response.Duration+'</span> </div> <div class="media-body"><div class="company-logo"> <span class="diploma"><i class="fa fa-graduation-cap"></i> Download Diploma</span> </div>  <h6>'+ response.JobTitle+'</h6> <p>'
                    + response.CompanyName  + '</p>  <p>' + response.Location + '</p>   <p class="margin-top-10"> '
                    +  response.Details + '</p>  </div>  </div>')
                $(".duration").val(" ");
                    $(".jobtitle").val(" ");
                    $(".companyname").val(" ");
                    $(".location").val(" ");
                    $(".details").val(" ");
            },
            error: function (response) {

            }

        })



    }

    </script>

    @*add category*@
    <div class="modal fade" id="CategoryModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLongTitle">Modal title</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="form-group">
                        <label for="newName">Name</label>
                        <input type="text" class="form-control" id="newName" aria-describedby="emailHelp" placeholder="Enter Category Name">
                        <p>@ViewBag.FillInput</p>
                    </div>
                    <button id="addNewCategory" onclick=" AddCategory()" class="btn btn-sm btn-primary">Add</button>
                    <button id="DeleteExistsCategory" onclick="DeleteExistsCategory()" class="btn btn-sm btn-primary">Delete</button>

                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-sm btn-secondary" data-dismiss="modal">Close</button>

                </div>
            </div>
        </div>
    </div>

    <div class="modal fade" id="TypeOfSkillsModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLongTitle" aria-hidden="true">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="exampleModalLongTitle">Modal title</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
